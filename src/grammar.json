{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "ass",
  "rules": {
    "source_file": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "section"
      }
    },
    "section": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "section_header"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT1",
              "content": {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "comment"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "key_value_pair"
                  }
                ]
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "section_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "SYMBOL",
          "name": "section_name"
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "section_name": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[^\\]]+"
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": ";"
          },
          {
            "type": "PATTERN",
            "value": "[^\\[\\r\\n]*"
          }
        ]
      }
    },
    "key_value_pair": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "key"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "value"
        }
      ]
    },
    "key": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[^:\\[\\r\\n]+"
      }
    },
    "value": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[^\\r\\n]+"
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}